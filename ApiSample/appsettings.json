{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "Connection": "127.0.0.1:6379"
  },
  "RateLimiter": {
    "RedisConfiguration": "127.0.0.1:6379",
    "DefaultAlgorithm": "Hybrid",
    "DefaultCalls": 5,
    "DefaultPeriodSeconds": 60,
    "DefaultScope": "PerIp",
    "FailOpen": false,
    "WarmUpInitialCapacity": 10
  }

}
